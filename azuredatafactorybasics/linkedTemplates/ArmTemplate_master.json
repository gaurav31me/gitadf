{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredatafactorybasics"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"SqlServerLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLinkedService'"
		},
		"ls_covid_db_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covid_db_sql'"
		},
		"ls_ecdc_dataset_lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ecdc_dataset_lake'"
		},
		"LS_AzureKeyVault_BLOB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://fornula1keyvault.vault.azure.net/"
		},
		"ls_ecdc_dataset_blob_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Blob-secrets"
		},
		"ls_ecdc_dataset_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_ecdc_dataset_lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingla.dfs.core.windows.net"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f8989928-b5d0-4b49-8b3e-719e39fb653e/resourceGroups/databricksfactoryRG/providers/Microsoft.Storage/storageAccounts/covidreportingsalake"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredatafactorybasics_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"ls_covid_db_sql_connectionString": {
						"value": "[parameters('ls_covid_db_sql_connectionString')]"
					},
					"ls_ecdc_dataset_lake_accountKey": {
						"value": "[parameters('ls_ecdc_dataset_lake_accountKey')]"
					},
					"LS_AzureKeyVault_BLOB_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_BLOB_properties_typeProperties_baseUrl')]"
					},
					"ls_ecdc_dataset_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_dataset_http_properties_typeProperties_url')]"
					},
					"ls_ecdc_dataset_lake_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_dataset_lake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "azuredatafactorybasics_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServerLinkedService_connectionString": {
						"value": "[parameters('SqlServerLinkedService_connectionString')]"
					},
					"ls_ecdc_dataset_blob_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_ecdc_dataset_blob_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azuredatafactorybasics_ArmTemplate_0')]"
			]
		},
		{
			"name": "azuredatafactorybasics_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azuredatafactorybasics_ArmTemplate_1')]"
			]
		}
	]
}